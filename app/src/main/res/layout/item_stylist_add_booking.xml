<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="100dp"
    android:layout_height="140dp"
    tools:ignore="MissingConstraints">

    <ImageView
        android:id="@+id/img_item_stylist_add_booking"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/avatar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cv_item_stylist_add_booking"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="Stylist"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/img_item_stylist_add_booking"
        app:layout_constraintStart_toStartOf="@+id/img_item_stylist_add_booking"
        app:layout_constraintTop_toBottomOf="@+id/img_item_stylist_add_booking" />
    <TextView
        android:id="@+id/hoten_item_stylist_add_booking"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="Phi de"
        android:textColor="@color/black"
        android:gravity="center"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/cv_item_stylist_add_booking"
        app:layout_constraintStart_toStartOf="@+id/cv_item_stylist_add_booking"
        app:layout_constraintTop_toBottomOf="@+id/cv_item_stylist_add_booking" />
</androidx.constraintlayout.widget.ConstraintLayout>